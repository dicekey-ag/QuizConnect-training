{
  "name": "quiz-connect-backend",
  "version": "0.1.0",
  "private": true,
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "sequelize": "^6.37.3"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "NODE_ENV=development node -r dotenv/config server.js dotenv_config_path=.env.development",
    "start:prod": "NODE_ENV=production node -r dotenv/config server.js dotenv_config_path=.env.production",
    "test": "jest",
    "test:dev": "NODE_ENV=development jest --runInBand --detectOpenHandles --forceExit",
    "test:integration": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit tests/integration",
    "test:e2e": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit tests/e2e",
    "migrate": "sequelize-cli db:migrate"
  },
  "main": "app.js",
  "keywords": [],
  "author": "",
  "description": "",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/integration/**/*.integration.test.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/tests/seeders/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
