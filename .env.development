# 共通
NODE_ENV=development

# データベース設定
DB_HOST=db
DB_USER=qc_user
DB_PASSWORD=qc_pass
DB_NAME=quizconnect_dev
DB_PORT=3306
DB_DIALECT=mysql

# テスト環境用
TEST_DB_NAME=quizconnect_test

# JWT設定
JWT_SECRET=your_development_jwt_secret_key

# MySQL root パスワード
MYSQL_ROOT_PASSWORD=root_password_for_development

# Frontend URL
FRONTEND_URL=http://localhost:3000

# API URL
REACT_APP_API_URL=http://localhost:5000/api

# .env.developmentに以下を追加

# セッションシークレットも追加（必要に応じて）
SESSION_SECRET=your_development_session_secret
